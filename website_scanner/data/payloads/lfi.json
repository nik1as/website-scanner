{
  "linux": {
    "separators": [
      [
        "..",
        "/"
      ],
      [
        "..",
        "//"
      ],
      [
        "..",
        "%2F"
      ],
      [
        "..",
        "%2F%2F"
      ],
      [
        "....",
        "//"
      ],
      [
        "..",
        "////"
      ]
    ],
    "files": [
      {
        "path": [
          "etc",
          "passwd"
        ],
        "content": [
          "root:x",
          "daemon:",
          "bin:",
          "sys:",
          "sync:",
          "games:"
        ]
      },
      {
        "path": [
          "etc",
          "group"
        ],
        "content": [
          "root:x:0:",
          "daemon:x:1:",
          "bin:x:2:",
          "sys:x:3:"
        ]
      },
      {
        "path": [
          "etc",
          "hosts"
        ],
        "content": [
          "127.0.0.1    localhost"
        ]
      }
    ]
  },
  "windows": {
    "separators": [
      [
        "..",
        "\\"
      ],
      [
        "..",
        "\\\\"
      ],
      [
        "..",
        "%5C"
      ],
      [
        "..",
        "%5C%5C"
      ]
    ],
    "files": [
      {
        "path": [
          "boot.ini"
        ],
        "content": [
          "screenEnabled",
          "[BOOTCTR9]",
          "splash = "
        ]
      },
      {
        "path": [
          "WINDOWS",
          "win.ini"
        ],
        "content": [
          "[mci extensions]",
          "adts=MPEGVideo"
        ]
      },
      {
        "path": [
          "WINDOWS",
          "system32",
          "drivers",
          "etc",
          "hosts"
        ],
        "content": [
          "127.0.0.1    localhost"
        ]
      }
    ]
  }
}